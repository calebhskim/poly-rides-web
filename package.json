{
  "name": "poly-rides-web",
  "version": "0.0.1",
  "description": "React-native-web implementation of the PolyRides app",
  "main": "index.js",
  "repository": "git@github.com:calebhskim/poly-rides-web.git",
  "author": "calebhskim <caleb.hs.kim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "build_scripts": "./node_modules/.bin/babel ./scripts/src -d ./scripts/build",
    "local": "yarn run build && ./node_modules/.bin/webpack-dev-server --content-base build/",
    "members": "yarn run build_scripts && node ./scripts/build/members.js",
    "mockdata": "yarn run build_scripts && node ./scripts/build/mockdata.js",
    "start": "yarn run build && node ./build/server.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.10.9"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-stage-1": "^6.22.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "firebase": "^3.6.4",
    "html-webpack-plugin": "^2.24.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.7",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-native": "^0.41.2",
    "react-native-web": "^0.0.69",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "readline": "^1.3.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-register": "^6.18.0",
    "clui": "^0.3.1",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-react-native": "^2.2.1",
    "firebase-mock": "^1.0.3",
    "firebase-server": "^0.9.1",
    "inquirer": "^2.0.0",
    "jest": "^19.0.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "redux-mock-store": "^1.2.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "!src/server.js",
      "src/**/*.{js,jsx}"
    ]
  }
}
